{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run example on main.dart",
            "program": "${workspaceFolder}/example/lib/main.dart",
            "request": "launch",
            "type": "dart",
            "args": [
                "--debug"
            ]
        },
        {
            "name": "Run on build_runner",
            "program": "${workspaceFolder}/example/.dart_tool/build/entrypoint/build.dart",
            "request": "launch",
            "type": "dart",
            "args": [
                "build",
                "--delete-conflicting-outputs",
            ]
        },
    ]
}